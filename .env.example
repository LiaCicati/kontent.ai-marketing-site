# Kontent.ai Environment ID (found in Project Settings > API keys)
KONTENT_PROJECT_ID=your_environment_id_here

# Preview Delivery API key (found in Project Settings > API keys)
KONTENT_PREVIEW_API_KEY=your_preview_api_key_here

# Management API key (found in Project Settings > API keys)
# Only needed for running the setup/seed script
KONTENT_MANAGEMENT_API_KEY=your_management_api_key_here

# Secret for preview mode webhook (any random string you choose)
KONTENT_PREVIEW_SECRET=your_preview_secret_here
